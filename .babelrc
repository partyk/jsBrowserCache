{
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": [
          "> 1% in CZ",
          "Explorer >= 9",
          "iOs >= 9",
          "last 3 versions"
        ]
      }
    }]
  ],
  "ignore": [
    "src/polifyllClosest.js"
  ],
  "plugins": [
    "@babel/plugin-syntax-object-rest-spread",
    "babel-plugin-loop-optimizer",
    ["@babel/plugin-transform-modules-umd", {
      "exactGlobals": false
    }]
  ]
}